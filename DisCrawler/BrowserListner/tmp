var parser = new DOMParser();
var xmlDoc = parser.parseFromString("<book><chapter/></book>", "text/xml");
var xmlDoc2 = parser.parseFromString("<yy><chapter/></yy>", "text/xml");
var node=xmlDoc.importNode(xmlDoc2.documentElement)
xmlDoc.documentElement.appendChlid(node);
var serializer = new XMLSerializer();
var xmlString = serializer.serializeToString(xmlDoc);
